<h1>Posts</h1>

<table class="table table-striped table-hover">
	<thead>
		<tr>
			<th>
				#
			</th>
			<th>
				Date
			</th>
			<th>
				User
			</th>
			<th>
				Rationale
			</th>
			<th>
				
			</th>
		</tr>
	</thead>
	<tbody>
		<% @posts.each do |post| %>
			<%= render "post", post: post %>
		<% end %>
	</tbody>
</table>